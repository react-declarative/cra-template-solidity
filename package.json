{
  "name": "cra-template-solidity",
  "version": "1.0.14",
  "description": "A Ethereum starter kit for rapid development of the proof of concept solidity app",
  "keywords": [
    "react",
    "create-react-app",
    "typescript",
    "mui",
    "ethers",
    "web3",
    "metamask",
    "solidity",
