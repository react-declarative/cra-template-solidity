    "material-ui",
    "mobx",
    "state-management",
    "form-validation",
    "layout-engine",
    "grid-system",
    "declarative-programming",
    "json-endpoint",
    "ux"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/react-declarative/cra-template-solidity/issues"
  },
  "homepage": "https://github.com/react-declarative/cra-template-solidity#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-declarative/cra-template-solidity.git"
  },
  "scripts": {
