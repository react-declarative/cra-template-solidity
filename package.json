    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.10.0"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.8",
    "@types/node": "16.11.51",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/react-window": "1.8.5",
    "@types/uuid": "8.3.4",
    "clsx": "1.2.1",
    "dayjs": "1.11.5",
    "ethers": "^5.7.2",
    "history": "5.3.0",
    "mobx": "6.6.2",
