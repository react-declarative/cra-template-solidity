  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/no-anonymous-default-export": "off",
      "new-parens": "off"
    }
  },
  "files": [
    "template",
    "template.json"
  ],
  "msw": {
    "workerDirectory": "template/public"
